{
  "name": "blog.nasustim.com",
  "version": "1.0.0",
  "private": true,
  "description": "blog.nasustim.com",
  "author": "nasustim",
  "scripts": {
    "fix": "bun run textlint:fix && bun run biome:fix",
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "clean": "rm -rf dist .astro",
    "biome:ci": "biome ci ./src",
    "biome:fix": "biome check --write --unsafe",
    "textlint:ci": "textlint \"src/content/**\"",
    "textlint:fix": "textlint \"src/content/**\" --fix",
    "new": "bun run ts -- scripts/create-new-article.ts",
    "ts": "node --experimental-strip-types --experimental-transform-types --experimental-detect-module --no-warnings=ExperimentalWarning"
  },
  "dependencies": {
    "@acab/reset.css": "0.11.0",
    "@astrojs/check": "0.9.5",
    "@astrojs/mdx": "4.3.9",
    "@astrojs/react": "4.4.1",
    "@astrojs/rss": "4.0.13",
    "@astrojs/sitemap": "3.6.0",
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-regular-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.0",
    "@mdx-js/react": "3.1.1",
    "@vanilla-extract/css": "1.17.4",
    "astro": "5.15.3",
    "clsx": "2.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "10.1.0",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.1",
    "ts-dedent": "github:nasustim/node-ts-dedent#master",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@types/node": "24.9.2",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "@vanilla-extract/vite-plugin": "5.1.1",
    "textlint": "15.2.3",
    "textlint-plugin-html": "1.0.1",
    "textlint-rule-common-misspellings": "1.0.1",
    "textlint-rule-preset-ja-spacing": "2.4.3",
    "typescript": "5.9.3"
  }
}
